{
  "applications": {
    "async_now": {
      "type": "external",
      "working_directory": "$pwd/target",
      "executable": "$pwd/target/scala-2.13/scala-native-out",
      "processes": 1,
      "limits": {
        "timeout": 1
      }
    }
  },

  "listeners": {
    "*:7575": {
      "pass": "applications/async_now"
    }
  }
}